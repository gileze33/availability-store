{
  "rules": {
    "class-name": true,
    "jsdoc-format": true,
    "indent": [true, "spaces"],
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "quotemark": [true, "single"],
    "radix": true,
    "trailing-comma": [true, "multiline"],
    "triple-equals": true
  }
}
